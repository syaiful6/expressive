{
    "name": "zendframework/zend-expressive-skeleton",
    "description": "Zend expressive skelton. Begin developing PSR-7 middleware applications in seconds",
    "type": "project",
    "homepage": "https://github.com/zendframework/zend-expressive-skeleton",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Geert Eltink",
            "homepage": "https://xtreamwayz.com/"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev",
            "dev-develop": "1.1-dev"
        }
    },
    "require": {
        "php": "^5.5 || ^7.0",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "roave/security-advisories": "dev-master",
        "zendframework/zend-expressive": "^1.0",
        "zendframework/zend-expressive-helpers": "^2.0",
        "zendframework/zend-stdlib": "^2.7 || ^3.0",
        "zendframework/zend-expressive-fastroute": "^1.0",
        "zendframework/zend-servicemanager": "^2.7.3 || ^3.0",
        "zendframework/zend-eventmanager": "^2.6 || ^3.0",
        "ocramius/proxy-manager": "^1.0 || ^2.0",
        "zendframework/zend-expressive-twigrenderer": "^1.0",
        "syaiful6/itertools": "~0.1",
        "syaiful6/headbanger": "~0.1",
        "illuminate/database": "^5.2",
        "illuminate/hashing": "^5.2",
        "illuminate/encryption": "^5.2",
        "illuminate/support": "^5.2",
        "symfony/translation": "2.8.*|3.0.*",
        "symfony/console": "^3.0",
        "symfony/finder": "2.8.*|3.0.*",
        "paragonie/random_compat": "~1.4",
        "symfony/process": "2.8.*|3.0.*",
        "vlucas/phpdotenv": "~2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "^2.3",
        "filp/whoops": "^1.1 || ^2.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "src/app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AppTest\\": "test/AppTest/"
        },
        "files": [
          "src/app/Foundation/utils.php"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "scripts": {
        "check": [
            "@cs",
            "@test"
        ],
        "cs": "phpcs",
        "cs-fix": "phpcbf",
        "serve": "php -S 0.0.0.0:8080 -t public/ public/index.php",
        "test": "phpunit"
    }
}
